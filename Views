views.py 
from django.shortcuts import render
from .models import CoffeeVarity
from .forms import CoffeeVarityForm


# Create your views here.

def all_coffee (request):
    form = CoffeeVarityForm()
    Varites = CoffeeVarity.objects.all()
    return render (request,'coffee/all_coffee.html', {'form': form, 'Varities': Varites}) 
